
{% extends "layout.twig" %}

{% block content %}

<div class="bk-pt-default">
      <header class="bk-header">
          <div class="header-container wrapper clearfix">
            <div class="companyheader clearfix">
                {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
                {{widget('companyname', 'companyname', {"business": "profile"})|raw}}
            </div>
          </div>
          {{widget('extendednavigation', 'navigationheader', {"align":"center"})|raw}}
      </header> 

<div class="canvas">
    <div class="page-wrap">
          <div class="wrapper clearfix">
              {{zone('main')|raw}}
          </div>
    </div>
</div>
              
<footer class="bk-footer">
    <div class="footer-container">
        {{widget('companyname', 'defaultcompanyname', {"business": "ATLANTIC"})|raw}}
        {{widget('companyaddress', 'defaultcompanyaddress', {})|raw}}
    </div>
</footer>

</div>

{% endblock %}